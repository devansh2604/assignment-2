#include <iostream>
using namespace std;

int main() {
    int arr[7] = {64, 34, 25, 12, 22, 11, 90};
    int n = 7, i, j, temp;
    for (i = 0; i < n - 1; i++)
        for (j = 0; j < n - i - 1; j++)
            if (arr[j] > arr[j+1]) {
                temp = arr[j]; arr[j] = arr[j+1]; arr[j+1] = temp;
            }

    cout << "Sorted: ";
    for (i = 0; i < n; i++) cout << arr[i] << " ";
    return 0;
}
