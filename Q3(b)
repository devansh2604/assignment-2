#include <iostream>
using namespace std;

int main() {
    int arr[5] = {1,2,4,5,6}; // missing 3
    int n = 5, low = 0, high = n - 1, mid;
    while (low <= high) {
        mid = (low + high) / 2;
        if (arr[mid] != mid + 1 && (mid == 0 || arr[mid-1] == mid)) {
            cout << "Missing: " << mid + 1; return 0;
        } else if (arr[mid] == mid + 1) low = mid + 1;
        else high = mid - 1;
    }
    cout << "No missing";
    return 0;
}
